{
    "name": "yaroslawww/nova-flexible-content-field-shortcode",
    "description": "Designed specifically for the ability to add shortcodes to existing content.",
    "keywords": [
        "laravel-nova",
        "nova",
        "nova-flexible-content",
        "shortcode"
    ],
    "homepage": "https://github.com/yaroslawww/nova-flexible-content-field-shortcode",
    "license": "MIT",
    "authors": [
        {
            "name": "Yaroslav Georgitsa",
            "email": "yaroslav.georgitsa@gmail.com",
            "homepage": "https://github.com/yaroslawww",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "ThinkOne\\NovaFlexibleContentFieldShortcode\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "ThinkOne\\NovaFlexibleContentFieldShortcode\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
